package bitmanip;

public class Tutorial {
	static int repeatedArithmeticShiftRight(int data,int count){
		//Shift data <count> times
		for(int i=0;i<count;i++)
			data>>=1;
		return data;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println(repeatedArithmeticShiftRight(-93242,12));
		System.out.println(Math.pow(2, 12));
	}

}
